<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.3.xsd">
                  


    <bean id="empbean" class="com.bean.Employee" >
        <property name="eid" value="100"></property>
        <property name="ename" value="Ram"></property>
        <property name="salary" value="22000"></property>
  	</bean>


  <!--  <context:annotation-config/>-->
    <!-- initializing CommonAnnotationBeanPostProcessor is same as context:annotation-config -->
  <!--  <bean class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor" />-->


</beans>